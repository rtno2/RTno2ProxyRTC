# RTno2ProxyRTC CMake config file
#
# This file sets the following variables:
# RTno2ProxyRTC_FOUND - Always TRUE.
# RTno2ProxyRTC_INCLUDE_DIRS - Directories containing the RTno2ProxyRTC include files.
# RTno2ProxyRTC_IDL_DIRS - Directories containing the RTno2ProxyRTC IDL files.
# RTno2ProxyRTC_LIBRARIES - Libraries needed to use RTno2ProxyRTC.
# RTno2ProxyRTC_DEFINITIONS - Compiler flags for RTno2ProxyRTC.
# RTno2ProxyRTC_VERSION - The version of RTno2ProxyRTC found.
# RTno2ProxyRTC_VERSION_MAJOR - The major version of RTno2ProxyRTC found.
# RTno2ProxyRTC_VERSION_MINOR - The minor version of RTno2ProxyRTC found.
# RTno2ProxyRTC_VERSION_REVISION - The revision version of RTno2ProxyRTC found.
# RTno2ProxyRTC_VERSION_CANDIDATE - The candidate version of RTno2ProxyRTC found.

message(STATUS "Found RTno2ProxyRTC-@PROJECT_VERSION@")
set(RTno2ProxyRTC_FOUND TRUE)

find_package(<dependency> REQUIRED)

#set(RTno2ProxyRTC_INCLUDE_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@"
#    ${<dependency>_INCLUDE_DIRS}
#    )
#
#set(RTno2ProxyRTC_IDL_DIRS
#    "@CMAKE_INSTALL_PREFIX@/include/@PROJECT_NAME_LOWER@-@PROJECT_VERSION_MAJOR@/idl")
set(RTno2ProxyRTC_INCLUDE_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@"
    ${<dependency>_INCLUDE_DIRS}
    )
set(RTno2ProxyRTC_IDL_DIRS
    "@CMAKE_INSTALL_PREFIX@/include/@CPACK_PACKAGE_FILE_NAME@/idl")


if(WIN32)
    set(RTno2ProxyRTC_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_STATIC_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
else(WIN32)
    set(RTno2ProxyRTC_LIBRARIES
        "@CMAKE_INSTALL_PREFIX@/@LIB_INSTALL_DIR@/@CMAKE_SHARED_LIBRARY_PREFIX@@PROJECT_NAME_LOWER@@CMAKE_SHARED_LIBRARY_SUFFIX@"
        ${<dependency>_LIBRARIES}
        )
endif(WIN32)

set(RTno2ProxyRTC_DEFINITIONS ${<dependency>_DEFINITIONS})

set(RTno2ProxyRTC_VERSION @PROJECT_VERSION@)
set(RTno2ProxyRTC_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(RTno2ProxyRTC_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(RTno2ProxyRTC_VERSION_REVISION @PROJECT_VERSION_REVISION@)
set(RTno2ProxyRTC_VERSION_CANDIDATE @PROJECT_VERSION_CANDIDATE@)

